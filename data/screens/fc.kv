ShowcaseScreen:
    name: "fc"

    BoxLayout:
        orientation: "vertical"

        FileChooserIconView:
            id: icon_view_tab
            multiselect: True

        BoxLayout:
            size_hint_y: .13
            padding: 10
            spacing: 30

            Button:
                text: "Open"
                on_release:
                    app.load(icon_view_tab.selection)
                    app.go_previous_screen()


            Button:
                text: "Cancel"
                on_release: app.go_previous_screen()
