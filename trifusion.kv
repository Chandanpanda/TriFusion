        halign: "left"
        valign: "middle"
        text_size: self.size
        color: (0.216, 0.67, 0.784, 1)
        size_hint_y: None
        height: 20

    BoxLayout:
        size_hint_y: None
        height: 80
        spacing: 5
        padding: 10
        orientation: "vertical"
        id: intra_aln_bx
        StackLayout:
            canvas.after:
                Color:
                    rgb: (0.216, 0.67, 0.784, 1)
                Line:
                    width: 1
                    cap: "round"
                    joint: "round"
                    rectangle: intra_aln_bx.x,intra_aln_bx.y,intra_aln_bx.width,intra_aln_bx.height

        Label:
