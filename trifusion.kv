#===============================================================================
# CHECK ACTIONS
#===============================================================================

<CheckBt@Button>:
    background_normal: "data/backgrounds/bt_process.png"
    bold: True
    shorten: True
    shorten_from: "right"
    text_size: self.size
    halign: "center"
    valign: "middle"
    StackLayout:
        canvas.after:
            Color:
                rgb: (1, .33, .33, 1)
            Line:
                joint: "miter"
                cap: "square"
                close: True
                width: 1.05
                rectangle: root.x,root.y,root.width,root.height

<CheckDialog>:
    orientation: "vertical"
    id: check
    text: None
    BoxLayout:
        padding: 10
        Label:
            valign: "middle"
